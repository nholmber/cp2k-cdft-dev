# With GCC 4.9 or later, for gcc and gfortran
CC       = gcc
CPP      = 
FC       = gfortran
LD       = gfortran
AR       = ar -r
RANLIB   = ranlib
DFLAGS   = -D__NO_STATM_ACCESS
FCFLAGS  = -fopenmp -ffast-math -funroll-loops -ftree-vectorize -ffree-form -O2 $(DFLAGS)
LDFLAGS  = $(FCFLAGS) 
LIBS     = -framework Accelerate
